{
    "manifest_version": 2,
    "name": "Minis Translator",
    "description": "This extension translates all Chinese text on screen",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["activeTab"]
}
